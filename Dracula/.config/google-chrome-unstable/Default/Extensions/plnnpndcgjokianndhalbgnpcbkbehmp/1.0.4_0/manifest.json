{
   "author": "apowersoft",
   "background": {
      "scripts": [ "js/utils/RecordRTC.js", "js/utils/getAllAudioVideoDevices.js", "js/background.common.js", "js/background.js" ]
   },
   "browser_action": {
      "default_icon": "img/logo.png",
      "default_popup": "popup.html",
      "default_title": "__MSG_name__"
   },
   "content_scripts": [ {
      "css": [ "css/content.css" ],
      "js": [ "js/content-script.js" ],
      "matches": [ "\u003Call_urls>" ],
      "run_at": "document_end"
   } ],
   "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
   "default_locale": "en",
   "description": "__MSG_description__",
   "icons": {
      "128": "img/logo.png",
      "16": "img/logo.png",
      "48": "img/logo.png"
   },
   "key": "",
   "manifest_version": 2,
   "minimum_chrome_version": "49",
   "name": "__MSG_name__",
   "permissions": [ "tabCapture", "activeTab", "desktopCapture", "notifications", "storage" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.0.4",
   "web_accessible_resources": [ "tab-camera-view.html", "js/tab-camera-view.js" ]
}
