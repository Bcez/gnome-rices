#cardContainer {
	padding:10px;
	padding-right:310px;
}

#cardContainerView {
	position:relative;
	margin-top:-200px;
	transition:margin-top 250ms ease-out;
}

#cardContainerView .albumCard {
	float:none!important;
	position:absolute;
	transition:top 250ms ease-out, left 250ms ease-out;
}


.albumCard {
	width:160px;
	height:160px;
	background-color:#dddddd;
	float:left;
	margin:10px;
	position:relative;
	background-size:160px 120px;
	border-radius:2px;
	overflow:hidden;
	box-shadow: 1px 1px 3px 1px #777777;
	background-size:42px 42px;
	background-repeat:no-repeat;
	background-position:50% 35%;
	background-image:url('loader.gif');

}

.albumCard .thumb {
	width: 160px;
	height: 120px;
	top: 0px;
	left: 0px;
	position: absolute;	
	background-repeat:no-repeat;
	background-position:50% 50%;
}

.albumCard .shade {
	width: 160px;
	height: 120px;
	top: 0px;
	left: 0px;
	position: absolute;	
	background-color:#e0e0e0;
	opacity:0;
	transition:opacity 500ms ease-out;
}


.albumCard.paused .shade {
	opacity:0.8;
}

.albumCard .empty {
	background-image:url('emptyfolder.png');
	background-size:72px;
	background-color:#eeeeee;
}



.albumCard .menu {
	margin-top:-25px;
	height:25px;
	background-color:rgba(255,255,255,0.7);
	position:absolute;
	top:0px;
	width:100%;
	/*font-family: sans-serif;*/

	transition:margin-top 100ms ease-out 100ms;
	font-size:12px;
	box-shadow: 0px 1px 4px 0px rgba(0, 0, 0, 0.38);
}

.albumCard .menu .action {
	cursor:pointer;
	width:25px;
	height:25px;
	float:left;
	background-position:50% 50%;
	background-repeat:no-repeat;
	opacity:.8;
	transition:all 150ms ease-out;
}

.albumCard .menu .action:hover {
	opacity:1;
	background-color:rgba(255,255,255,0.3);
}

.albumCard .menu .disabled {
	opacity:.3;
	cursor:default;
}

.albumCard .menu .disabled:hover {
	opacity:.3!important;
	cursor:default;
	background-color:rgba(0,0,0,0);
}

.albumCard .menu .castnow {
	background-image:url('castnow.png');
	background-size:16px 16px;
}

.albumCard .menu .delete {
	background-image:url('trash.png');
	background-size:16px 16px;
}

.albumCard .menu .pause {
	background-image:url('pause.png');
	background-size:16px 16px;
}

.albumCard .menu .unpause {
	background-image:url('play.png');
	background-size:16px 16px;
	display:none;
}

.albumCard.paused .pause {
	display:none;
}

.albumCard.paused .unpause {
	display:block;
}

.albumCard:hover .menu {
	margin-top:0;
	transition:margin-top 100ms ease-out;
}


.albumCard .info {
	height:40px;
	background-color:white;
	position:absolute;
	bottom:0px;
	width:100%;
	/*font-family: sans-serif;*/
	font-size:12px;
	box-shadow: 0px -1px 7px 0px rgba(0, 0, 0, 0.38);
}

.albumCard .castwait {
	background-image:url('images/castanim.gif');	
	background-size: 16px 16px;
	background-repeat: no-repeat;
	background-position: 97% 80%;
}

.albumCard .info .title {
	font-weight:bold;
	margin:4px;
	overflow: hidden;
	white-space: nowrap;
	text-overflow: ellipsis;	
}

.albumCard .info .total {
	margin:4px;
	margin-top:-2px;
	font-size:11px;
	color:#777777;
}

