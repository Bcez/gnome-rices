{
   "app": {
      "background": {
         "scripts": [ "google-analytics-bundle.js", "background.js" ]
      }
   },
   "description": "A plugin that enables Remote Control in AirDroid Web",
   "externally_connectable": {
      "accepts_tls_channel_id": false,
      "matches": [ "http://*.airdroid.com/*", "https://*.airdroid.com/*" ]
   },
   "icons": {
      "128": "icon.png",
      "16": "icon.png",
      "48": "icon.png"
   },
   "key": "",
   "manifest_version": 2,
   "minimum_chrome_version": "40",
   "name": "AirDroid Remote Control Plugin",
   "permissions": [ "nativeMessaging", "unlimitedStorage", "notifications", "https://www.google-analytics.com/", "storage", {
      "fileSystem": [ "write", "retainEntries", "directory" ]
   }, "usb", "http://*.airdroid.com/", "https://*.airdroid.com/" ],
   "sockets": {
      "tcp": {
         "connect": "*"
      },
      "tcpServer": {
         "listen": "*"
      }
   },
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.9.8"
}
